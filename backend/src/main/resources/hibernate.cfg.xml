<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>       
  <session-factory>
    <property name="dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="show_sql">true</property>
    <property name="hibernate.hbm2ddl.auto">update</property>
    <mapping class="ch.zhaw.psit4.martin.db.HistoryItem"/>
    <mapping class="ch.zhaw.psit4.martin.db.Response"/>
    <mapping class="ch.zhaw.psit4.martin.db.Request"/>
    <mapping class="ch.zhaw.psit4.martin.db.ExampleCall"/>
    <mapping class="ch.zhaw.psit4.martin.db.Parameter"/>
    <mapping class="ch.zhaw.psit4.martin.db.Keyword"/>
    <mapping class="ch.zhaw.psit4.martin.db.Function"/>
    <mapping class="ch.zhaw.psit4.martin.db.Author"/>
    <mapping class="ch.zhaw.psit4.martin.db.Plugin"/>
  </session-factory>
</hibernate-configuration>